{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": [
            "ignore-case"
        ],
        "interface-name": false,
        "max-line-length": [true, 240],
        "jsx-no-multiline-js": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}